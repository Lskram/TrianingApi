@ControllAPI_HostAddress = http://localhost:5091/api

// Get all users
GET {{ControllAPI_HostAddress}}/User/
Accept: application/json

###
// Get user by ID
GET {{ControllAPI_HostAddress}}/User/4
Accept: application/json

###
// Create a new user
POST {{ControllAPI_HostAddress}}/User
Content-Type: application/json
Accept: application/json

